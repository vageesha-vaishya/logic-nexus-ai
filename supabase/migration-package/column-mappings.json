{
  "currencies": {
    "map": {
      "code": "code",
      "name": "name",
      "symbol": "symbol",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["code", "tenant_id"],
    "on_conflict": ["code", "tenant_id"],
    "drop": ["created_at", "updated_at", "id"]
  },
  "service_types": {
    "map": {
      "code": "code",
      "name": "name",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["code", "tenant_id"],
    "on_conflict": ["code", "tenant_id"],
    "drop": ["description", "created_at", "updated_at", "id"]
  },
  "package_categories": {
    "map": {
      "category_name": "name",
      "category_code": "code",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["code", "tenant_id"],
    "dedupe_key": ["code", "tenant_id"],
    "drop": ["created_at", "updated_at", "id"]
  },
  "package_sizes": {
    "map": {
      "size_name": "name",
      "max_weight_kg": "weight_kg",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["name", "tenant_id"],
    "dedupe_key": ["name", "tenant_id"],
    "drop": ["created_at", "updated_at", "id", "size_code", "length_ft", "width_ft", "height_ft", "description"]
  },
  "container_sizes": {
    "map": {
      "length_ft": "size_feet",
      "max_weight_kg": "max_weight_kg",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "derive": {
      "size_feet": "from_name_or_code"
    },
    "match_on": ["size_feet", "tenant_id"],
    "on_conflict": ["size_feet", "tenant_id"],
    "drop": ["created_at", "updated_at", "id", "name", "code", "width_ft", "height_ft", "description"]
  },
  "container_types": {
    "map": {
      "name": "name",
      "code": "code",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["code", "tenant_id"],
    "on_conflict": ["code", "tenant_id"],
    "drop": ["created_at", "updated_at", "id"]
  },
  "ports_locations": {
    "map": {
      "location_name": "name",
      "location_code": "code",
      "location_type": "port_type",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "derive_fk": {
      "country_id": {
        "from": "country",
        "lookup_table": "countries",
        "lookup_key": "name",
        "lookup_target": "id",
        "aliases": {
          "US": "United States",
          "U.S.": "United States",
          "USA": "United States",
          "U.S.A.": "United States",
          "UK": "United Kingdom",
          "U.K.": "United Kingdom",
          "UAE": "United Arab Emirates",
          "KSA": "Saudi Arabia",
          "Brasil": "Brazil",
          "Viet Nam": "Vietnam",
          "Cote d'Ivoire": "Côte d’Ivoire"
        }
      },
      "city_id": {
        "from": "city",
        "lookup_table": "cities",
        "lookup_key": "name",
        "lookup_target": "id",
        "aliases": {
          "NYC": "New York",
          "LA": "Los Angeles",
          "SFO": "San Francisco",
          "SJ": "San Jose",
          "Dxb": "Dubai",
          "Abu Dhabi": "Abu Dhabi",
          "HCM": "Ho Chi Minh City",
          "Saigon": "Ho Chi Minh City",
          "Bkk": "Bangkok"
        }
      }
    },
    "match_on": ["code", "tenant_id"],
    "on_conflict": ["code", "tenant_id"],
    "drop": [
      "coordinates",
      "facilities",
      "operating_hours",
      "country",
      "city",
      "state_province",
      "postal_code",
      "customs_available",
      "notes",
      "created_at",
      "updated_at",
      "id"
    ]
  },
  "cargo_types": {
    "map": {
      "cargo_type_name": "name",
      "cargo_code": "code",
      "requires_special_handling": "is_hazardous",
      "temperature_controlled": "requires_temperature_control",
      "description": "description",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "match_on": ["code", "tenant_id"],
    "dedupe_key": ["code", "tenant_id"],
    "drop": ["created_at", "updated_at", "id"]
  },
  "service_type_mappings": {
    "map": {
      "service_id": "external_code",
      "is_active": "is_active",
      "tenant_id": "tenant_id"
    },
    "set": {
      "external_system": "core"
    },
    "derive_fk": {
      "service_type_id": {
        "from": "service_type",
        "lookup_table": "service_types",
        "lookup_key": "code",
        "lookup_target": "id",
        "aliases": {
          "air": "air_freight",
          "ocean": "ocean_freight",
          "courier": "courier_service"
        }
      }
    },
    "match_on": ["external_system", "external_code", "service_type_id", "tenant_id"],
    "dedupe_key": ["external_system", "external_code", "service_type_id", "tenant_id"],
    "drop": ["conditions", "service_type", "is_default", "priority", "created_at", "updated_at", "id"]
  },
  
  "tenants": {
    "map": {
      "id": "id",
      "name": "name",
      "slug": "slug",
      "logo_url": "logo_url",
      "domain": "domain",
      "subscription_tier": "subscription_tier",
      "is_active": "is_active",
      "created_at": "created_at",
      "updated_at": "updated_at"
    },
    "match_on": ["slug"],
    "on_conflict": ["slug"],
    "drop": ["settings", "stripe_customer_id", "billing_email", "payment_method", "billing_address"]
  },
  "franchises": {
    "map": {
