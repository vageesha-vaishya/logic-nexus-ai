    "derive_fk": {
      "tenant_id": {
        "from": "tenant_slug",
        "lookup_table": "tenants",
        "lookup_key": "slug",
        "lookup_target": "id"
      }
    },
    "match_on": ["code"],
    "on_conflict": ["code"],
    "drop": ["address", "manager_id"]
  },
  "accounts": {
