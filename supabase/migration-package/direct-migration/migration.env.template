# Source Database (e.g. Local Trae Dev or Legacy Lovable)
SOURCE_DB_URL="postgresql://postgres:postgres@localhost:54322/postgres"

# Target Database (Supabase Cloud)
TARGET_DB_URL="postgresql://postgres:[PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres"

# Migration Options
MIGRATE_SCHEMA=true
MIGRATE_DATA=true
MIGRATE_AUTH=true
MIGRATE_STORAGE=false

# Batch Size for Data Sync (if using script)
BATCH_SIZE=1000
